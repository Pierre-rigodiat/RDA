    <!ENTITY exp "http://www.nist.gov/itl/ssd/Experiments#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY oil "http://www.daml.org/2001/03/daml+oil#" >
    <!ENTITY ont "http://www.co-ode.org/ontologies/ont.owl#" >
    <!ENTITY or "http://www.nist.gov/itl/ssd/OntologyRequirements#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
]>


<rdf:RDF xmlns="http://www.nist.gov/itl/ssd/Experiments#"
     xml:base="http://www.nist.gov/itl/ssd/Experiments#"
     xmlns:bc="http://www.nist.gov/itl/ssd/Biochem"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:doc="http://www.nist.gov/itl/ssd/Document#"
     xmlns:exp="http://www.nist.gov/itl/ssd/Experiments#"
     xmlns:or="http://www.nist.gov/itl/ssd/OntologyRequirements#"
     xmlns:oil="http://www.daml.org/2001/03/daml+oil#"
     xmlns:ont="http://www.co-ode.org/ontologies/ont.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
    <owl:Ontology rdf:about="http://www.nist.gov/itl/ssd/Experiments">
        <dc:date xml:lang="en">12 October 2012</dc:date>
        <ont:creator>doug foxvog</ont:creator>
        <dc:title xml:lang="en">Experiments Ontology</dc:title>
        <dc:subject xml:lang="en">Ontology for general concepts related to experiments.  </dc:subject>
        <skos:note xml:lang="en">This ontology is incomplete, having been developed to cover general features of experiments in several different fields.  Imaging and imagee processing concepts are included.</skos:note>
        <owl:imports rdf:resource="http://www.nist.gov/itl/ssd/Biochem"/>
        <owl:imports rdf:resource="http://www.nist.gov/itl/ssd/Document"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#defaultFileExtension"/>
    <owl:AnnotationProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfProperty"/>
    <owl:AnnotationProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#specializedTypeOf"/>
    <owl:AnnotationProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#catalogNumber"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    




    <!-- http://www.nist.gov/itl/ssd/Experiments#hasUncertaintyDistributionType -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#hasUncertaintyDistributionType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The property `has Uncertainty Distribution Type' relates an Uncertainty to its Uncertainty Distribution Type, e.g., Gaussian Distribution.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Uncertainty"/>
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#UncertaintyDistributionType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#imageProducedByStudy -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#imageProducedByStudy">
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#ExperimentalStudy"/>
        <rdfs:domain rdf:resource="&or;Image"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#imagingDeviceUsedInStudy -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#imagingDeviceUsedInStudy">
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#ExperimentalStudy"/>
        <rdfs:domain rdf:resource="&or;ImagingDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#investigator -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#investigator">
        <rdfs:comment xml:lang="en">Any researcher for a research study.  Use the property &#39;principal Investigator&#39; if appropriate.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Research"/>
        <rdfs:range rdf:resource="&or;Human"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#measurementResult -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#measurementResult">
        <rdfs:comment xml:lang="en">The property &#39;measurement Result&#39; relates an act of Measuring to an Expressed Quantity.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Measuring"/>
        <rdfs:range rdf:resource="&or;ExpressedQuantity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#measurementTechniqueUsed -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#measurementTechniqueUsed">
        <rdfs:comment xml:lang="en">The property &#39;measurement Technique Used&#39; relates an act of Measuring something to the Measurement Technique that it implemented.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#MeasurementTechnique"/>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Measuring"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#numOfSignificantDigits -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#numOfSignificantDigits">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The property &#39;num Of Significant Digits&#39; specifies the Number Of Significant Digits of a measurement or computed value.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#NumberOfSignificantDigits"/>
        <rdfs:subPropertyOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfQuantity"/>
        <rdfs:domain rdf:resource="&or;ExpressedQuantity"/>
    </owl:ObjectProperty>
    




    <!-- http://www.nist.gov/itl/ssd/Experiments#principalInvestigator -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#principalInvestigator">
        <rdfs:comment xml:lang="en">The designer of a research study.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Research"/>
        <rdfs:subPropertyOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#investigator"/>
        <rdfs:range rdf:resource="&or;Human"/>
    </owl:ObjectProperty>
    
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#researchUsesSoftware -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#researchUsesSoftware">
        <rdfs:comment xml:lang="en">The specified research uses the specified software for data processing.  If it merely is performing research on the software, this predicate does not apply.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Research"/>
        <rdfs:range rdf:resource="&or;Software"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#standardDeviation -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#standardDeviation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The standard Deviation of an Uncertainty is a quantity with the same units as the Uncertainty itself.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Uncertainty"/>
        <rdfs:range rdf:resource="&or;Quantity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#technician -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#technician">
        <rdfs:comment xml:lang="en">The person who performs a technical task as part of some scientific study or similar technical activity.</rdfs:comment>
        <rdfs:range rdf:resource="&or;Human"/>
        <rdfs:domain rdf:resource="&or;PurposefulAction"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfProperty -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfProperty">
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Uncertainty"/>
        <rdfs:domain rdf:resource="&or;MaterialProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfQuantity -->

    <owl:ObjectProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfQuantity">
        <rdfs:comment xml:lang="en">The property &#39;uncertainty Of Quantity&#39; relates a Quantity to its associated Uncertainty.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Uncertainty"/>
        <rdfs:domain rdf:resource="&or;MeasurableQuantity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/OntologyRequirements#implementsAlgorithm -->

    <owl:ObjectProperty rdf:about="&or;implementsAlgorithm"/>
    


    <!-- http://www.nist.gov/itl/ssd/OntologyRequirements#outputData -->

    <owl:ObjectProperty rdf:about="&or;outputData"/>
    


    <!-- http://www.nist.gov/itl/ssd/OntologyRequirements#subEvents -->

    <owl:ObjectProperty rdf:about="&or;subEvents"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.nist.gov/itl/ssd/Experiments#assayChamberTemperatureC -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#assayChamberTemperatureC">
        <rdfs:comment xml:lang="en">The temperature (in degrees C) of the Assay Chamber in an Experiment.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Experiment"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    

    <!-- http://www.nist.gov/itl/ssd/Experiments#lotNumber -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#lotNumber">
        <rdfs:comment xml:lang="en">The lot number of some product.</rdfs:comment>
        <rdfs:domain rdf:resource="&or;Artifact"/>
        <rdfs:subPropertyOf rdf:resource="&or;nameString"/>
        <rdfs:range rdf:resource="&xsd;NMTOKEN"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#passBandCenterNM -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#passBandCenterNM">
        <rdfs:comment xml:lang="en">The property `pass Band Center NM&#39; specifies the center of a Band Pass Light Filter in nanometers.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#BandPassLightFilter"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#passBandHalfWidthNM -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#passBandHalfWidthNM">
        <rdfs:comment xml:lang="en">The property `pass Band Half Width NM&#39; specifies the half-width in nanometers of a Band Pass Light Filter.  I.e., the band is the pass Band Center NM +/- the half-width.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#BandPassLightFilter"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    

    <!-- http://www.nist.gov/itl/ssd/Experiments#productLotNumber -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#productLotNumber">
        <rdfs:comment>A `product Lot Number' is a code that designates the place and time of origin of some physical product which it shares with similar products produced around the same time and location, whether the product is an artifact or an agricultureal product.</rdfs:comment>
        <rdfs:domain rdf:resource="&or;PhysicalObject"/>
        <rdfs:range rdf:resource="&xsd;NMTOKEN"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#receiptDate -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#receiptDate">
        <rdfs:comment xml:lang="en">The `receipt Date&#39; of some material is the date it was received from the source.</rdfs:comment>
        <rdfs:domain rdf:resource="&or;PhysicalObject"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    



    <!-- http://www.nist.gov/itl/ssd/Experiments#storageTemeratureInCelsius -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#storageTemeratureInCelsius">
        <rdfs:comment xml:lang="en">The temperature at which frozen cells were stored (in Celsius).</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#CellCulture"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#supplySourceName -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#supplySourceName">
        <rdfs:comment xml:lang="en">The `supply Source Name&#39; of some object is the name of the source of the supply.  This may be the name (or acronym) of the vendor or the location where the item / material was obtained.</rdfs:comment>
        <rdfs:domain rdf:resource="&or;PhysicalObject"/>
        <rdfs:range rdf:resource="&xsd;NMTOKEN"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#uncertaintyConfidenceLevel -->

    <owl:DatatypeProperty rdf:about="http://www.nist.gov/itl/ssd/Experiments#uncertaintyConfidenceLevel">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Uncertainty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.nist.gov/itl/ssd/OntologyRequirements#referenceURI -->

    <owl:DatatypeProperty rdf:about="&or;referenceURI"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.nist.gov/itl/ssd/Document#MakingAPrediction -->

    <rdf:Description rdf:about="&doc;MakingAPrediction">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Analysis"/>
    </rdf:Description>
    


    <!-- http://www.nist.gov/itl/ssd/Document#RefiningAConcept -->

    <rdf:Description rdf:about="&doc;RefiningAConcept">
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Experiment"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Measuring"/>
    </rdf:Description>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#Analysis -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#Analysis">
        <rdfs:subClassOf rdf:resource="&or;PurposefulAction"/>
        <rdfs:comment xml:lang="en">Analysis is the Purposeful Action of considering something in detail in order to understand some of its features. </rdfs:comment>
    </owl:Class>
    



    <!-- http://www.nist.gov/itl/ssd/Experiments#ArtificialLens -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#ArtificialLens">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Lens"/>
        <rdfs:subClassOf rdf:resource="&or;PhysicalDevice"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#BeamSplitter"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Incubator"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#MicroscopeStage"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#RadiationOutputDevice"/>
        <owl:disjointWith rdf:resource="&or;Computer"/>
        <owl:disjointWith rdf:resource="&or;ImagingDevice"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BaconianExperiment -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BaconianExperiment">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#PhysicalExperiment"/>
        <rdfs:seeAlso xml:lang="en">Medawar, P.B. Advice to a Young Scientist. Pan Books Ltd, London, 1981</rdfs:seeAlso>
        <rdfs:seeAlso>http://www.nationmaster.com/encyclopedia/EXPOpropositions#Baconian_experiment</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">A Baconian experiment is a scientific experiment which involves no explicit hypothesis.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BandPassEmissionFilter -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BandPassEmissionFilter">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#BandPassLightFilter"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BandPassExcitationFilter -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BandPassExcitationFilter">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#BandPassLightFilter"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BandPassImageFilteringAlgorithm -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BandPassImageFilteringAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#ImageFilteringAlgorithm"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#HighPassImageFilteringAlgorithm"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#LowPassImageFilteringAlgorithm"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BandPassLightFilter -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BandPassLightFilter">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#LightFilter"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BeamSplitter -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BeamSplitter">
        <rdfs:subClassOf rdf:resource="&or;LaboratoryEquipment"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Incubator"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#LightFilter"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#LightShutter"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#MicroscopeStage"/>
        <owl:disjointWith rdf:resource="&or;Computer"/>
        <owl:disjointWith rdf:resource="&or;ImagingDevice"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#BiologicalMaterialPreparationStep -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#BiologicalMaterialPreparationStep">
        <rdfs:subClassOf rdf:resource="&or;PurposefulAction"/>
    </owl:Class>
    



    <!-- http://www.nist.gov/itl/ssd/Experiments#CCDCamera -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#CCDCamera">
        <rdfs:subClassOf rdf:resource="&or;Camera"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#CalculationMathematical -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#CalculationMathematical">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Analysis"/>
        <rdfs:comment xml:lang="en">The mathemeatical calculation of a result from one or more equations</rdfs:comment>
        <or:termString xml:lang="en">calculation</or:termString>
        <or:termString xml:lang="en">mathematical calculation</or:termString>
    </owl:Class>
    



    <!-- http://www.nist.gov/itl/ssd/Experiments#ComputationalExperiment -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#ComputationalExperiment">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Experiment"/>
        <owl:disjointWith rdf:resource="http://www.nist.gov/itl/ssd/Experiments#PhysicalExperiment"/>
        <rdfs:comment xml:lang="en">A Computational Experiment is a scientific experiment which investigates cause-effect relations between known and unknown (target) variables by manipulating the computational (non-physical) domain adequate to the real-world domain.</rdfs:comment>
        <rdfs:seeAlso>http://www.nationmaster.com/encyclopedia/EXPOpropositions#computational_experiment</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#ComputationalModeling -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#ComputationalModeling">
        <rdfs:subClassOf rdf:resource="&or;Modeling"/>
        <rdfs:comment xml:lang="en">Computational Modeling is the process of intentionally creating a Computational Model.</rdfs:comment>
    </owl:Class>
    



    <!-- http://www.nist.gov/itl/ssd/Experiments#DataAnalysis -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#DataAnalysis">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#Analysis"/>
        <rdfs:comment xml:lang="en">Data Analysis is the Analysis of a set of data</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Experiments#EdgeDetectionAlgorithm -->

    <owl:Class rdf:about="http://www.nist.gov/itl/ssd/Experiments#EdgeDetectionAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.nist.gov/itl/ssd/Experiments#ImageProcessingAlgorithm"/>
