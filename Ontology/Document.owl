    <!-- http://www.nist.gov/itl/ssd/Document#DatabaseEntry -->

    <owl:Class rdf:about="&doc;DatabaseEntry">
        <rdfs:subClassOf rdf:resource="&or;AbstractStructuredInformationStore"/>
        <owl:disjointWith rdf:resource="&doc;Prediction"/>
