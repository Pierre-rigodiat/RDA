    <!-- http://www.nist.gov/itl/ssd/Document#authorOf -->

    <owl:ObjectProperty rdf:about="&doc;authorOf">
        <rdfs:comment xml:lang="en">The &#39;author Of&#39; a Document, System Model, or other piece of information is a person or organization which created the work.  A work may have  more than one author.</rdfs:comment>
        <rdfs:domain rdf:resource="&or;AbstractStructuredInformationStore"/>
        <rdfs:range rdf:resource="&or;IntelligentAgent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Document#publisherOf -->

    <owl:ObjectProperty rdf:about="&doc;publisherOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The relation &#39;publisher Of&#39; specifies the Organization which originally published a Document, System Model, or other such work.</rdfs:comment>
        <rdfs:domain rdf:resource="&Document;PublishedInformation"/>
        <rdfs:range rdf:resource="&or;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#doiLink -->

    <owl:ObjectProperty rdf:about="&Document;doiLink">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">A digital object identifier (DOI) is a character string (a &quot;digital identifier&quot;) used to uniquely identify an object such as an electronic document. Metadata about the object is stored in association with the DOI name and this metadata may include a location, such as a URL, where the object can be found. The DOI for a document is permanent, whereas its location and other metadata may change. Referring to an online document by its DOI provides more stable linking than simply referring to it by its URL, because if its URL changes, the publisher need only update the metadata for the DOI to link to the new URL</rdfs:comment>
        <rdfs:domain rdf:resource="&doc;Document"/>
        <rdfs:range rdf:resource="&Document;DigitalObjectIdentifier"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#issueOfPeriodical -->

    <owl:ObjectProperty rdf:about="&Document;issueOfPeriodical">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:range rdf:resource="&Document;Periodical"/>
        <rdfs:domain rdf:resource="&Document;PeriodicalIssue"/>
        <rdfs:subPropertyOf rdf:resource="&Document;subWorkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#publicationDate -->

    <owl:ObjectProperty rdf:about="&Document;publicationDate">
        <rdfs:comment xml:lang="en">The publication Date of some Published Information is the Date Time that it was officially published, even if copies were first distributed before or after that date. The publication Date may be given as a Calendar Day, a Calendar Month, a Calendar Quarter, a Calendar Year, or possibly at some other degree of granularity.</rdfs:comment>
        <rdfs:domain rdf:resource="&Document;PublishedInformation"/>
        <rdfs:range rdf:resource="&or;DateTime"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#specificModelOfGenericType -->

    <owl:ObjectProperty rdf:about="&Document;specificModelOfGenericType">
        <rdfs:comment xml:lang="en">The property &#39;specific Model Of Generic Type&#39; relates a Specific System Model to a Generic System Model of which it is a more specific form.</rdfs:comment>
        <rdfs:range rdf:resource="&doc;GenericSystemModel"/>
        <rdfs:domain rdf:resource="&doc;SpecificSystemModel"/>
    </owl:ObjectProperty>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#subWorkOf -->

    <owl:ObjectProperty rdf:about="&Document;subWorkOf">
        <rdfs:comment xml:lang="en">One Data Object is a subWork of another if all the data in the first is present in the same form in the second Data Object.</rdfs:comment>
        <rdfs:range rdf:resource="&OntologyRequirements;DataObject"/>
        <rdfs:domain rdf:resource="&OntologyRequirements;DataObject"/>
    </owl:ObjectProperty>
    


