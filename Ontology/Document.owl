    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#Periodical -->

    <owl:Class rdf:about="&Document;Periodical">
        <rdfs:subClassOf rdf:resource="&Document;PublishedCompilation"/>
        <rdfs:comment xml:lang="en">A periodically published work with multiple articles in each issue.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#PeriodicalIssue -->

    <owl:Class rdf:about="&Document;PeriodicalIssue">
        <rdfs:subClassOf rdf:resource="&doc;Document"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/Document.owl#PeriodicalVolume -->

    <owl:Class rdf:about="&Document;PeriodicalVolume">
        <rdfs:subClassOf rdf:resource="&Document;Periodical"/>
        <rdfs:comment xml:lang="en">A Periodical Volume is a series of continuous issues of the same Periodical grouped into a logical volume by the issuer of the periodical.  There may be one or two volumes a year, with each issue of the periodical being an issue of the volume for the same time period and each issue of a periodical volume being an issue of the periodical, as well.</rdfs:comment>
    </owl:Class>
    


