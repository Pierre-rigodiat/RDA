        <rdfs:subClassOf rdf:resource="&mp;SpectroscopicAnalysis"/>
        <owl:disjointWith rdf:resource="&mp;ElectronProbeMicroanalysis"/>
        <owl:disjointWith rdf:resource="&mp;NuclearReactionAnalysis"/>
        <owl:disjointWith rdf:resource="&mp;Resistometry"/>
        <owl:disjointWith rdf:resource="&mp;RutherfordBackscattering"/>
        <owl:disjointWith rdf:resource="&mp;SecondaryIonMassSpectroscopy"/>
        <owl:disjointWith rdf:resource="&mp;XrayDiffractionAnalysis"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#BinaryPhaseDiagram -->

    <owl:Class rdf:about="&mp;BinaryPhaseDiagram">
        <rdfs:subClassOf rdf:resource="&mp;PhaseDiagram"/>
        <owl:disjointWith rdf:resource="&mp;HigherOrderPhaseDiagram"/>
        <owl:disjointWith rdf:resource="&mp;PressureTemperaturePhaseDiagram"/>
        <owl:disjointWith rdf:resource="&mp;TernaryPhaseDiagram"/>
        <rdfs:comment xml:lang="en">A Binary Phase Diagram is a Phase Diagram for a two component system.  The most common binary phase diagrams are  Temperature-Concentration Phase Diagrams at a constant pressure.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#BondingMaterials -->

    <owl:Class rdf:about="&mp;BondingMaterials">
        <rdfs:subClassOf rdf:resource="&mp;AttachingMaterials"/>
        <rdfs:seeAlso xml:lang="en">Bonding Materials is the process of forming a permanent bond between materials, e.g., by welding, soldering, or gluing.</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#Brazing -->

    <owl:Class rdf:about="&mp;Brazing">
        <rdfs:subClassOf rdf:resource="&mp;BondingMaterials"/>
        <owl:disjointWith rdf:resource="&mp;IonBeamSputtering"/>
        <owl:disjointWith rdf:resource="&mp;Soldering"/>
        <owl:disjointWith rdf:resource="&mp;Welding"/>
        <rdfs:comment xml:lang="en">Brazing is a metal-joining process whereby a filler metal is heated above melting point and distributed between two or more close-fitting parts by capillary action. The filler metal is brought slightly above its melting (liquidus) temperature while protected by a suitable atmosphere, usually a flux. It then flows over the base metal (known as wetting) and is then cooled to join the workpieces together.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#CalculationFromHeatingCoolingCurve -->

    <owl:Class rdf:about="&mp;CalculationFromHeatingCoolingCurve">
        <rdfs:subClassOf rdf:resource="&exp;CalculationMathematical"/>
        <or:referenceURL rdf:datatype="&xsd;anyURI">http://www.iupac.org/namespaces/ThermoML</or:referenceURL>
        <rdfs:comment xml:lang="en">Calculation of a value or result from a heating or cooling curve</rdfs:comment>
        <or:termString xml:lang="en">heating/cooling curve</or:termString>
        <or:termString xml:lang="en">heating/cooling curves</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#Calorimetry -->

    <owl:Class rdf:about="&mp;Calorimetry">
        <rdfs:subClassOf rdf:resource="&exp;Measuring"/>
        <owl:disjointWith rdf:resource="&mp;DifferentialThermalAnalysis"/>
        <owl:disjointWith rdf:resource="&mp;Spectroscopy"/>
        <owl:disjointWith rdf:resource="&mp;SurfaceCounting"/>
        <rdfs:comment xml:lang="en">Calorimetry is the act of measuring the heat of chemical reactions or physical changes.</rdfs:comment>
        <or:termString xml:lang="en">calorimetry</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#Casting_MaterialProcessing -->

    <owl:Class rdf:about="&mp;Casting_MaterialProcessing">
        <rdfs:subClassOf rdf:resource="&mp;ShapingMaterial"/>
        <owl:disjointWith rdf:resource="&mp;ChannelDieCompression"/>
        <owl:disjointWith rdf:resource="&mp;ColdCompaction"/>
        <owl:disjointWith rdf:resource="&mp;Extrusion"/>
        <owl:disjointWith rdf:resource="&mp;Forging_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;HeatTreating"/>
        <owl:disjointWith rdf:resource="&mp;Hydroforming"/>
        <owl:disjointWith rdf:resource="&mp;Quenching"/>
        <owl:disjointWith rdf:resource="&mp;Rolling_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;Sectioning"/>
        <owl:disjointWith rdf:resource="&mp;SheetForming"/>
        <owl:disjointWith rdf:resource="&mp;Sintering"/>
        <rdfs:comment xml:lang="en">In metalworking, casting involves pouring liquid metal into a mold, which contains a hollow cavity of the desired shape, and then allowing it to cool and solidify. The solidified part is also known as a casting, which is ejected or broken out of the mold to complete the process. Casting is most often used for making complex shapes that would be difficult or uneconomical to make by other methods.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#ChannelDieCompression -->

    <owl:Class rdf:about="&mp;ChannelDieCompression">
        <rdfs:subClassOf rdf:resource="&mp;ShapingMaterial"/>
        <owl:disjointWith rdf:resource="&mp;ColdCompaction"/>
        <owl:disjointWith rdf:resource="&mp;Extrusion"/>
        <owl:disjointWith rdf:resource="&mp;Forging_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;HeatTreating"/>
        <owl:disjointWith rdf:resource="&mp;Hydroforming"/>
        <owl:disjointWith rdf:resource="&mp;Quenching"/>
        <owl:disjointWith rdf:resource="&mp;Rolling_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;Sectioning"/>
        <owl:disjointWith rdf:resource="&mp;SheetForming"/>
        <owl:disjointWith rdf:resource="&mp;Sintering"/>
        <rdfs:seeAlso xml:lang="en">Francillette et al, Acta Materialia, Volume 46, Issue 12, 24 July 1998, Pages 4131–4142 http://www.sciencedirect.com/science/article/pii/S1359645498001219</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">In Channel Die Compression, a sample is placed in a rectangular channel with one side open.  The sample is compressed from the open side and expansion is allowed in both directions along the channel, but restricted in the transverse direction.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#Chromatography -->

    <owl:Class rdf:about="&mp;Chromatography">
        <rdfs:subClassOf rdf:resource="&mp;MixtureSeparation"/>
        <rdfs:comment xml:lang="en">Chromatography is a laboratory technique for separating mixtures dissolved or suspended in a fluid.  The various constituents of the fluid mixture travel at different rates through another material causing them to separate.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#ColdCompaction -->

    <owl:Class rdf:about="&mp;ColdCompaction">
        <rdfs:subClassOf rdf:resource="&mp;ShapingMaterial"/>
        <owl:disjointWith rdf:resource="&mp;Extrusion"/>
        <owl:disjointWith rdf:resource="&mp;Forging_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;HeatTreating"/>
        <owl:disjointWith rdf:resource="&mp;Hydroforming"/>
        <owl:disjointWith rdf:resource="&mp;Quenching"/>
        <owl:disjointWith rdf:resource="&mp;Rolling_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;Sectioning"/>
        <owl:disjointWith rdf:resource="&mp;SheetForming"/>
        <owl:disjointWith rdf:resource="&mp;Sintering"/>
        <rdfs:comment xml:lang="en">Cold Compaction is the process of pressing fine powdered materials into a desired shape or form, removing gaps from within and expelling gas from the powder without heating.  If the powder is a mixture of multiple substances, they would normally be blended very well before compaction.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#CompositionMeasuringThroughAutoradiography -->

    <owl:Class rdf:about="&mp;CompositionMeasuringThroughAutoradiography">
        <rdfs:subClassOf rdf:resource="&mp;RadiotracerCompositionMeasuring"/>
        <owl:disjointWith rdf:resource="&mp;RadiotracerCompositionMeasuringThroughResidualActivity"/>
        <owl:disjointWith rdf:resource="&mp;RadiotracerCompositionMeasuringThroughSerialSectioning"/>
        <owl:disjointWith rdf:resource="&mp;RadiotracerCompositionMeasuringThroughSurfaceActivity"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#ContinuousCasting -->

    <owl:Class rdf:about="&mp;ContinuousCasting">
        <rdfs:subClassOf rdf:resource="&mp;Casting_MaterialProcessing"/>
        <rdfs:comment xml:lang="en">Continuous casting, also called strand casting, is the process whereby molten metal is solidified into a &quot;semifinished&quot; billet, bloom, or slab for subsequent rolling in the finishing mills. Molten metal is poured into an open-ended, water-cooled copper mold, which allows a skin of solid metal to form over the still-liquid centre. The strand, as it is now called, is withdrawn from the mold and passed into a chamber of rollers and water sprays; the rollers support the thin skin of the strand while the sprays remove heat from the strand, gradually solidifying the strand from the outside in. After solidification, predetermined lengths of the strand are cut off.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#CoolingDevice -->

    <owl:Class rdf:about="&mp;CoolingDevice">
        <rdfs:subClassOf rdf:resource="&or;PhysicalDevice"/>
        <rdfs:comment xml:lang="en">A Cooling Device is a device for intentionally cooling some type of material down.  A device that turns off a Heating Device or allows generated heat to escape is not considered a cooling device.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#CorrelationGasChromatography -->

    <owl:Class rdf:about="&mp;CorrelationGasChromatography">
        <rdfs:subClassOf rdf:resource="&mp;GasChromatography"/>
        <or:referenceURL rdf:datatype="&xsd;anyURI">http://www.iupac.org/namespaces/ThermoML</or:referenceURL>
        <or:termString xml:lang="en">correlation gas chromatography</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#CrystallographicData -->

    <owl:Class rdf:about="&mp;CrystallographicData">
        <rdfs:subClassOf rdf:resource="&or;DataObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&or;storesInformationAbout"/>
                <owl:someValuesFrom rdf:resource="&mat;CrystalStructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&mp;PhaseEquilibriumData"/>
        <owl:disjointWith rdf:resource="&mp;ThermochemicalData"/>
        <owl:disjointWith rdf:resource="&mp;ThermodynamicData"/>
        <rdfs:comment xml:lang="en">Crystallographic Data is a Data Object that stores crystallographic information.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#DSC_DTA -->

    <owl:Class rdf:about="&mp;DSC_DTA">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&mp;DifferentialScanningCalorimetry"/>
                    <rdf:Description rdf:about="&mp;DifferentialThermalAnalysis"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&exp;Measuring"/>
        <or:referenceURL rdf:datatype="&xsd;anyURI">http://www.iupac.org/namespaces/ThermoML</or:referenceURL>
        <or:termString xml:lang="en">DSC/DTA</or:termString>
        <rdfs:comment xml:lang="en">Differential scanning calorimetry or differential thermal analysis</rdfs:comment>
        <or:termString xml:lang="en">differential thermal analysis or differential scanning calorimetry</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#DifferentialScanningCalorimetry -->

    <owl:Class rdf:about="&mp;DifferentialScanningCalorimetry">
        <rdfs:subClassOf rdf:resource="&mp;DSC_DTA"/>
        <rdfs:subClassOf rdf:resource="&mp;FlowCalorimetry"/>
        <owl:disjointWith rdf:resource="&mp;DropCalorimetry"/>
        <or:termString xml:lang="en">DSC</or:termString>
        <rdfs:comment xml:lang="en">Differential Scanning Calorimetry measures the heat flow difference between a sample and a reference material (subjected to the same temperature variation in a controlled atmosphere) to determine the temperature and heat of transformation.</rdfs:comment>
        <or:termString xml:lang="en">differential scanning calorimetry</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#DifferentialThermalAnalysis -->

    <owl:Class rdf:about="&mp;DifferentialThermalAnalysis">
        <rdfs:subClassOf rdf:resource="&mp;DSC_DTA"/>
        <owl:disjointWith rdf:resource="&mp;Spectroscopy"/>
        <owl:disjointWith rdf:resource="&mp;SurfaceCounting"/>
        <or:termString xml:lang="en">DTA</or:termString>
        <rdfs:comment xml:lang="en">Differential Thermal Analysis measures, the temperature difference between a sample and a reference material (subjected to the same temperature variation in a controlled atmosphere).</rdfs:comment>
        <or:termString xml:lang="en">differential thermal analysis</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#DiffractionData -->

    <owl:Class rdf:about="&mp;DiffractionData">
