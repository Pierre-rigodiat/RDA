        <owl:disjointWith rdf:resource="&mp;DifferentialThermalAnalysis"/>
        <owl:disjointWith rdf:resource="&mp;Spectroscopy"/>
        <owl:disjointWith rdf:resource="&mp;SurfaceCounting"/>
        <rdfs:comment xml:lang="en">Atomic Composition Measuring is the action of measuring the atomic composition of some object.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#AttachingMaterials -->

    <owl:Class rdf:about="&mp;AttachingMaterials">
        <rdfs:subClassOf rdf:resource="&mp;MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;Casting_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;ChannelDieCompression"/>
        <owl:disjointWith rdf:resource="&mp;ColdCompaction"/>
        <owl:disjointWith rdf:resource="&mp;HeatTreating"/>
        <owl:disjointWith rdf:resource="&mp;MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;RemovingMaterial"/>
        <owl:disjointWith rdf:resource="&mp;Sectioning"/>
        <rdfs:comment xml:lang="en">Attaching Materials is the process of physically connecting two materials so that they remain attached.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#AttachingWithConnector -->

    <owl:Class rdf:about="&mp;AttachingWithConnector">
        <rdfs:subClassOf rdf:resource="&mp;AttachingMaterials"/>
        <owl:disjointWith rdf:resource="&mp;BondingMaterials"/>
        <owl:disjointWith rdf:resource="&mp;ColdCompaction"/>
        <owl:disjointWith rdf:resource="&mp;Extrusion"/>
        <owl:disjointWith rdf:resource="&mp;Forging_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;Hydroforming"/>
        <owl:disjointWith rdf:resource="&mp;Rolling_MaterialProcessing"/>
        <owl:disjointWith rdf:resource="&mp;SheetForming"/>
        <owl:disjointWith rdf:resource="&mp;Sintering"/>
        <rdfs:comment xml:lang="en">Attaching With Connector is a process of connecting two or more objects together with a connecting object (bolt, screw, nail, staple, lace, etc.) that in most types of attaching penetrates both objects.  Removing the connecting object would cause the connected objects to be no longer connected.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#AugerSpectroscopy -->

    <owl:Class rdf:about="&mp;AugerSpectroscopy">
