        <owl:disjointWith rdf:resource="&or;ComputerImageFile"/>
        <rdfs:comment xml:lang="en">A file which maintains diffusion data.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#DiffusionMatrixTable -->

    <owl:Class rdf:about="&mp;DiffusionMatrixTable">
        <rdfs:subClassOf rdf:resource="&or;DataTable"/>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#DiffusionMobilityDatabase -->

    <owl:Class rdf:about="&mp;DiffusionMobilityDatabase">
        <rdfs:subClassOf rdf:resource="&mp;ThermodynamicDatabase"/>
