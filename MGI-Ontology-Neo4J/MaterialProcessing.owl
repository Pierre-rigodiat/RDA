    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#AlloyDesigning -->

    <owl:Class rdf:about="&mp;AlloyDesigning">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&mp;MaterialsDesigning"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&exp;outputCreated"/>
