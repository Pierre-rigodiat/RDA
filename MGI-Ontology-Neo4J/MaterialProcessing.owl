        <rdfs:comment xml:lang="en">Adiabatic Calorimetry is calorimetry using an ``adiabatic calorimeter&#39;&#39;.</rdfs:comment>
        <or:termString xml:lang="en">adiabatic calorimetry</or:termString>
    </owl:Class>
    


    <!-- http://www.nist.gov/itl/ssd/MaterialProcessing#AlloyDesign -->

    <owl:Class rdf:about="&mp;AlloyDesign">
