        <simpleType>
            <restriction base="integer">
                <minInclusive value="0"/>
                <maxInclusive value="100"/>
            </restriction>
        </simpleType>
        <unique name="un5">
            <selector xpath="id5"/>
            <field xpath="@id5"/>
        </unique>
    </element>
</schema>
