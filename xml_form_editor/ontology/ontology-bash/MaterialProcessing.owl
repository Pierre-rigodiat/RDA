###  http://www.nist.gov/itl/ssd/Experiments#outputCreated

exp:outputCreated rdf:type owl:ObjectProperty .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#hadProcessingStep

:hadProcessingStep rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment "The property 'had Processing Step' relates a Material Object to a Material Processing Type that was performed as it was being produced/prepared."@en ;
                   
                   rdfs:range :MaterialProcessingType ;
                   
                   rdfs:domain mat:MaterialObject .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.nist.gov/itl/ssd/Document.n3#ExtrapolationOfData

doc:ExtrapolationOfData owl:disjointWith exp:Experiment .



###  http://www.nist.gov/itl/ssd/Experiments#Experiment

exp:Experiment rdf:type owl:Class .



###  http://www.nist.gov/itl/ssd/Experiments#ScanningProbeMicroscope

exp:ScanningProbeMicroscope rdfs:subClassOf <http://www.nist.gov/itl/ssd/OntologyRequirements#Profilometer> .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#AlloyDesign

:AlloyDesign rdf:type owl:Class ;
             
             rdfs:subClassOf :MaterialsDesign .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#AlloyDesigning

:AlloyDesigning rdf:type owl:Class ;
                
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:intersectionOf ( :MaterialsDesigning
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty exp:outputCreated ;
                                                             owl:someValuesFrom :AlloyDesign
                                                           ]
                                                         )
                                    ] ;
                
                rdfs:subClassOf :MaterialsDesigning ;
                
                rdfs:comment "Alloy Designing is the activity of designing a new type of alloy"@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#Annealing

:Annealing rdf:type owl:Class ;
           
           rdfs:subClassOf :HeatTreating ;
           
           owl:disjointWith :ArcMelting ,
                            :Quenching ;
           
           rdfs:comment "Annealing is a heat treatment wherein a material is altered, causing changes in its properties such as strength and hardness. It is a process that produces conditions by heating to above the critical temperature, maintaining a suitable temperature, and then cooling."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#ArcMelting

:ArcMelting rdf:type owl:Class ;
            
            rdfs:subClassOf :HeatTreating ;
            
            rdfs:comment "Arc Melting is the process of melting a substance with the use of an electric arck. This may or may not be performed in an Electric Arc Furnace."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#AtomicCompositionMeasuring

:AtomicCompositionMeasuring rdf:type owl:Class ;
                            
                            rdfs:subClassOf exp:Measuring ;
                            
                            rdfs:comment "Atomic Composition Measuring is the action of measuring the atomic composition of some object."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#AugerSpectroscopy

:AugerSpectroscopy rdf:type owl:Class ;
                   
                   rdfs:subClassOf :DirectCompositionMeasuring ,
                                   :SpectroscopicAnalysis ;
                   
                   owl:disjointWith :ElectronProbeMicroanalysis ,
                                    :NuclearReactionAnalysis ,
                                    :Resistometry ,
                                    :RutherfordBackscattering ,
                                    :SecondaryIonMassSpectroscopy ,
                                    :XrayDiffractionAnalysis .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#BinaryPhaseDiagram

:BinaryPhaseDiagram rdf:type owl:Class ;
                    
                    rdfs:subClassOf :PhaseDiagram ;
                    
                    owl:disjointWith :HigherOrderPhaseDiagram ,
                                     :PressureTemperaturePhaseDiagram ,
                                     :TernaryPhaseDiagram ;
                    
                    rdfs:comment "A Binary Phase Diagram is a Phase Diagram for a two component system.  The most common binary phase diagrams are  Temperature-Concentration Phase Diagrams at a constant pressure."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#Casting_MaterialProcessing

:Casting_MaterialProcessing rdf:type owl:Class ;
                            
                            rdfs:subClassOf <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#ShapingMaterial> ;
                            
                            owl:disjointWith :ChannelDieCompression ,
                                             :ColdCompaction ,
                                             :Extrusion ,
                                             :Forging_MaterialProcessing ,
                                             :HeatTreating ,
                                             :Hydroforming ,
                                             :Quenching ,
                                             :Rolling_MaterialProcessing ,
                                             :Sectioning ,
                                             :SheetForming ,
                                             :Sintering ,
                                             <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#AttachingMaterials> ;
                            
                            rdfs:comment "In metalworking, casting involves pouring liquid metal into a mold, which contains a hollow cavity of the desired shape, and then allowing it to cool and solidify. The solidified part is also known as a casting, which is ejected or broken out of the mold to complete the process. Casting is most often used for making complex shapes that would be difficult or uneconomical to make by other methods."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#ChannelDieCompression

:ChannelDieCompression rdf:type owl:Class ;
                       
                       rdfs:subClassOf <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#ShapingMaterial> ;
                       
                       owl:disjointWith :ColdCompaction ,
                                        :Extrusion ,
                                        :Forging_MaterialProcessing ,
                                        :HeatTreating ,
                                        :Hydroforming ,
                                        :Quenching ,
                                        :Rolling_MaterialProcessing ,
                                        :Sectioning ,
                                        :SheetForming ,
                                        :Sintering ,
                                        <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#AttachingMaterials> ;
                       
                       rdfs:seeAlso "Francillette et al, Acta Materialia, Volume 46, Issue 12, 24 July 1998, Pages 4131â€“4142 http://www.sciencedirect.com/science/article/pii/S1359645498001219"@en ;
                       
                       rdfs:comment "In Channel Die Compression, a sample is placed in a rectangular channel with one side open.  The sample is compressed from the open side and expansion is allowed in both directions along the channel, but restricted in the transverse direction."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#ColdCompaction

:ColdCompaction rdf:type owl:Class ;
                
                rdfs:subClassOf <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#ShapingMaterial> ;
                
                owl:disjointWith :Extrusion ,
                                 :Forging_MaterialProcessing ,
                                 :HeatTreating ,
                                 :Hydroforming ,
                                 :Quenching ,
                                 :Rolling_MaterialProcessing ,
                                 :Sectioning ,
                                 :SheetForming ,
                                 :Sintering ,
                                 <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#AttachingMaterials> ,
                                 <http://www.nist.gov/itl/ssd/MaterialProcessing.owl#AttachingWithConnector> ;
                
                rdfs:comment "Cold Compaction is the process of pressing fine powdered materials into a desired shape or form, removing gaps from within and expelling gas from the powder without heating.  If the powder is a mixture of multiple substances, they would normally be blended very well before compaction."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#CompositionMeasuringThroughAutoradiography

:CompositionMeasuringThroughAutoradiography rdf:type owl:Class ;
                                            
                                            rdfs:subClassOf :RadiotracerCompositionMeasuring ;
                                            
                                            owl:disjointWith :RadiotracerCompositionMeasuringThroughResidualActivity ,
                                                             :RadiotracerCompositionMeasuringThroughSerialSectioning ,
                                                             :RadiotracerCompositionMeasuringThroughSurfaceActivity .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#CoolingDevice

:CoolingDevice rdf:type owl:Class ;
               
               rdfs:subClassOf <http://www.nist.gov/itl/ssd/OntologyRequirements#PhysicalDevice> ;
               
               rdfs:comment "A Cooling Device is a device for intentionally cooling some type of material down.  A device that turns off a Heating Device or allows generated heat to escape is not considered a cooling device."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#CrystallographicData

:CrystallographicData rdf:type owl:Class ;
                      
                      rdfs:subClassOf <http://www.nist.gov/itl/ssd/OntologyRequirements#DataObject> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://www.nist.gov/itl/ssd/OntologyRequirements#storesInformationAbout> ;
                                        owl:someValuesFrom mat:CrystalStructure
                                      ] ;
                      
                      owl:disjointWith :PhaseEquilibriumData ,
                                       :ThermochemicalData ,
                                       :ThermodynamicData ;
                      
                      rdfs:comment "Crystallographic Data is a Data Object that stores crystallographic information."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#DiffractionData

:DiffractionData rdf:type owl:Class ;
                 
                 rdfs:subClassOf <http://www.nist.gov/itl/ssd/OntologyRequirements#DataObject> ;
                 
                 owl:disjointWith :PhaseEquilibriumData ,
                                  :ThermochemicalData .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#DiffusionDataFile

:DiffusionDataFile rdf:type owl:Class ;
                   
                   rdfs:subClassOf <http://www.nist.gov/itl/ssd/OntologyRequirements#ComputerFile> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.nist.gov/itl/ssd/OntologyRequirements#storesInformationAbout> ;
                                     owl:someValuesFrom mat:Diffusion
                                   ] ;
                   
                   owl:disjointWith :MolarVolumeDataFile ,
                                    :ThermodynamicDataFile ,
                                    <http://www.nist.gov/itl/ssd/OntologyRequirements#ComputerImageFile> ;
                   
                   rdfs:comment "A file which maintains diffusion data."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#DiffusionMatrixTable

:DiffusionMatrixTable rdf:type owl:Class ;
                      
                      rdfs:subClassOf <http://www.nist.gov/itl/ssd/OntologyRequirements#DataTable> .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#DiffusionMobilityDatabase

:DiffusionMobilityDatabase rdf:type owl:Class ;
                           
                           rdfs:subClassOf :ThermodynamicDatabase ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.nist.gov/itl/ssd/OntologyRequirements#storesInformationAbout> ;
                                             owl:someValuesFrom mat:Diffusion
                                           ] ;
                           
                           rdfs:comment "A Diffusion Mobility Database is a Data Base with diffusion mobility data."@en .



###  http://www.nist.gov/itl/ssd/MaterialProcessing#DirectCompositionMeasuring

:DirectCompositionMeasuring rdf:type owl:Class ;
                            
                            rdfs:subClassOf :AtomicCompositionMeasuring ;
                            
                            owl:disjointWith :IndirectCompositionMeasuring ;
                            
                            rdfs:comment "Direct Composition Measuring is a type of measuring the composition of an object through direct measurment of the component species."@en .



