###  http://www.nist.gov/itl/ssd/Document#cODEN

:cODEN rdf:type owl:DatatypeProperty ;
       
       rdfs:comment "The CODEN id for a document"@en ;
       
       rdfs:domain :Document ;
       
       rdfs:subPropertyOf or:idString ;
       
       rdfs:range xsd:Name .



###  http://www.nist.gov/itl/ssd/Document#copyrightYear

:copyrightYear rdf:type owl:DatatypeProperty ;
               
               rdfs:domain :Document ;
               
               rdfs:range xsd:integer .



###  http://www.nist.gov/itl/ssd/Document#doi

:doi rdf:type owl:DatatypeProperty ;
     
     rdfs:domain :Document ;
     
     rdfs:subPropertyOf or:idString ;
     
     rdfs:range xsd:Name .



###  http://www.nist.gov/itl/ssd/Document#iSBN

:iSBN rdf:type owl:DatatypeProperty ;
      
      rdfs:comment "The International Standard Book Number (ISBN) maps a commercial book to a iunique 9-digit identifier."@en ;
      
      rdfs:domain :Book ;
      
      rdfs:subPropertyOf or:idString ;
      
      rdfs:range xsd:Name .



###  http://www.nist.gov/itl/ssd/Document#iSSN

:iSSN rdf:type owl:DatatypeProperty ;
      
      rdfs:comment "The International Standard Subscription Number of a document."@en ;
      
      rdfs:domain :Document ;
      
      rdfs:subPropertyOf or:idString ;
      
      rdfs:range xsd:Name .



###  http://www.nist.gov/itl/ssd/Document#patentNumber

:patentNumber rdf:type owl:DatatypeProperty ;
              
              rdfs:subPropertyOf or:idString .



###  http://www.nist.gov/itl/ssd/Document#publicationYear

:publicationYear rdf:type owl:DatatypeProperty ;
                 
                 rdfs:domain :Document ;
                 
                 rdfs:range xsd:integer .



###  http://www.nist.gov/itl/ssd/Document#volumeNumber

:volumeNumber rdf:type owl:DatatypeProperty ;
              
              rdfs:domain :DocumentSeries ;
              
              rdfs:range xsd:integer .



###  http://www.nist.gov/itl/ssd/Document.n3#chapterNumber

<http://www.nist.gov/itl/ssd/Document.n3#chapterNumber> rdf:type owl:DatatypeProperty ;
                                                        
                                                        rdfs:comment "The property ``chapter Number'' references the number (or, if appropriate, name) of a chapter that is referenced."@en ;
                                                        
                                                        rdfs:domain :Document ;
                                                        
                                                        rdfs:range xsd:string .



###  http://www.nist.gov/itl/ssd/Document.n3#editionNumber

<http://www.nist.gov/itl/ssd/Document.n3#editionNumber> rdf:type owl:DatatypeProperty ;
                                                        
                                                        rdfs:comment "The property ``edition Number'' specifies the edition number of a publication such as a book, specification, etc..  The value is normally a number, but specifications, for example might indicate sub-editions with something other than an integer."@en ;
                                                        
                                                        rdfs:domain :Document ;
                                                        
                                                        rdfs:range xsd:string .



###  http://www.nist.gov/itl/ssd/Document.n3#onPages

<http://www.nist.gov/itl/ssd/Document.n3#onPages> rdf:type owl:DatatypeProperty ;
                                                  
                                                  rdfs:comment "The property ``on Pages'' lists the page or page range of a Document in a Document Compilation."@en ;
                                                  
                                                  rdfs:domain :Document ;
                                                  
                                                  rdfs:range xsd:string .





