                                          <http://purl.org/dc/elements/1.1/date> "12 October 2012" ;
                                          
                                          <http://www.co-ode.org/ontologies/ont.owl#creator> "doug foxvog" ;
                                          
                                          <http://purl.org/dc/elements/1.1/title> "Document Ontology"@en ;
                                          
                                          <http://purl.org/dc/elements/1.1/subject> "Ontology for basic documentation concepts."@en ;
                                          
                                          <http://www.w3.org/2004/02/skos/core#note> "This ontology is incomplete, having been developed to cover general features of experiments to be used in recording information stored in genomic and materials science files."@en ;
                                          
