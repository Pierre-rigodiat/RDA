###  http://www.nist.gov/itl/ssd/Document#degreeIn

:degreeIn rdf:type owl:ObjectProperty ;
          
          rdfs:comment "The predicate ``degreeIn'' relates an academic degree to its field of study.  A single degree may be in multiple fields of study, such as a bachelor's degree in electrical engineering and computer science."@en ;
          
          rdfs:domain :AcademicDegree ;
          
          rdfs:range or:FieldOfStudy .



###  http://www.nist.gov/itl/ssd/Document#hasAuthor

:hasAuthor rdf:type owl:ObjectProperty ;
           
           rdfs:comment "The relation dc:author relates the first argument to the name of the second argument.  See http://www.iupac.org/namespaces/ThermoML#sAuthor."@en ;
           
           rdfs:subPropertyOf :hasContributor ;
           
           rdfs:domain or:AbstractStructuredInformationStore ;
           
           rdfs:range or:IntelligentAgent .



###  http://www.nist.gov/itl/ssd/Document#hasBinder

:hasBinder rdf:type owl:ObjectProperty ;
           
           rdfs:comment "The property hasBinder links a work to a person or organization that binds physical copies of the work"@en ;
           
           rdfs:subPropertyOf :hasContributor ;
           
           rdfs:domain or:AbstractStructuredInformationStore ;
           
           rdfs:range or:IntelligentAgent .



###  http://www.nist.gov/itl/ssd/Document#hasContributor

:hasContributor rdf:type owl:ObjectProperty ;
                
                rdfs:comment "The relation dc:contributor relates the first argument to the name of the second argument."@en ;
                
                rdfs:domain or:AbstractStructuredInformationStore ;
                
                rdfs:range or:IntelligentAgent .



###  http://www.nist.gov/itl/ssd/Document#hasEditor

:hasEditor rdf:type owl:ObjectProperty ;
           
           rdfs:subPropertyOf :hasContributor ;
           
           rdfs:domain or:AbstractStructuredInformationStore ;
           
           rdfs:range or:Human .



###  http://www.nist.gov/itl/ssd/Document#hasIllustrator

:hasIllustrator rdf:type owl:ObjectProperty ;
                
                rdfs:subPropertyOf :hasContributor ;
                
                rdfs:domain or:AbstractStructuredInformationStore ;
                
                rdfs:range or:IntelligentAgent .



###  http://www.nist.gov/itl/ssd/Document#hasPublisher

:hasPublisher rdf:type owl:ObjectProperty ;
              
              rdfs:subPropertyOf :hasContributor ;
              
              rdfs:domain or:AbstractStructuredInformationStore ;
              
              rdfs:range or:Organization .



###  http://www.nist.gov/itl/ssd/Document#issueOfPeriodical

:issueOfPeriodical rdf:type owl:FunctionalProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:comment "The property ``issueOfPeriodical'' relates a Journal Issue to the Periodical Series of which it is an issue."@en ;
                   
                   rdfs:domain :JournalIssue ;
                   
                   rdfs:range :PeriodicalSeries .



###  http://www.nist.gov/itl/ssd/Document#languageOfDocument

:languageOfDocument rdf:type owl:ObjectProperty ;
                    
                    rdfs:domain :Document ;
                    
                    rdfs:range or:Language .



###  http://www.nist.gov/itl/ssd/Document#proofreader

:proofreader rdf:type owl:ObjectProperty ;
             
             rdfs:subPropertyOf :hasContributor ;
             
             rdfs:domain or:AbstractStructuredInformationStore ;
             
             rdfs:range or:Human .



###  http://www.nist.gov/itl/ssd/Document#publicationMonth

:publicationMonth rdf:type owl:ObjectProperty ;
                  
                  rdfs:domain :Document ;
                  
                  rdfs:range or:MonthOfYearType .



###  http://www.nist.gov/itl/ssd/Document#reviewedBy

:reviewedBy rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf :hasContributor ;
            
            rdfs:domain or:AbstractStructuredInformationStore ;
            
            rdfs:range or:Human .



###  http://www.nist.gov/itl/ssd/Document#translatedBy

:translatedBy rdf:type owl:ObjectProperty ;
              
              rdfs:comment "The property translatedBy links a work to a person or organization that translated the work into another language.  A ternary relation or reification of either the translated work or the act of translation would be necessary to link the translator to language into which the work was translated."@en ;
              
              rdfs:subPropertyOf :hasContributor ;
              
              rdfs:domain or:AbstractStructuredInformationStore ;
              
              rdfs:range or:IntelligentAgent .



###  http://www.nist.gov/itl/ssd/Document#translatedInto

:translatedInto rdf:type owl:ObjectProperty ;
                
                rdfs:comment "The property translatedInto specifies a language into which a work was translated.  Note that a work may be translated into multiple languages."@en ;
                
                rdfs:domain or:AbstractStructuredInformationStore ;
                
                rdfs:range or:Language .



###  http://www.nist.gov/itl/ssd/Document.n3#depictsInformationAbout

<http://www.nist.gov/itl/ssd/Document.n3#depictsInformationAbout> rdf:type owl:ObjectProperty ;
                                                                  
                                                                  rdfs:comment "The property 'depicts Information About' is used to relate an Image to the thing(s) that it maintains information about."@en ;
                                                                  
                                                                  rdfs:subPropertyOf <http://www.nist.gov/itl/ssd/Document.n3#providesInformationAbout> ;
                                                                  
                                                                  rdfs:domain or:Image ,
                                                                              or:Information .



###  http://www.nist.gov/itl/ssd/Document.n3#modelsInformationAbout

<http://www.nist.gov/itl/ssd/Document.n3#modelsInformationAbout> rdf:type owl:ObjectProperty ;
                                                                 
                                                                 rdfs:comment "The property 'models Information About' is used to relate a model to the thing(s) that it maintains information about."@en ;
                                                                 
                                                                 rdfs:domain :SystemModel ;
                                                                 
                                                                 rdfs:subPropertyOf <http://www.nist.gov/itl/ssd/Document.n3#providesInformationAbout> .



###  http://www.nist.gov/itl/ssd/Document.n3#providesInformationAbout

<http://www.nist.gov/itl/ssd/Document.n3#providesInformationAbout> rdf:type owl:ObjectProperty ;
                                                                   
                                                                   rdfs:comment "The property provides Information About' is used to relate a piece of information to the thing(s) that it maintains information about."@en ;
                                                                   
                                                                   rdfs:domain or:Information .



###  http://www.nist.gov/itl/ssd/Document.n3#specificationAuthority

<http://www.nist.gov/itl/ssd/Document.n3#specificationAuthority> rdf:type owl:ObjectProperty ;
                                                                 
                                                                 rdfs:comment "The property specification Authority relates a Specification to the Authority Organization that maintains the specification."@en ;
                                                                 
                                                                 rdfs:range <http://www.nist.gov/itl/ssd/Document.n3#AuthorityOrganization> ;
                                                                 
                                                                 rdfs:domain <http://www.nist.gov/itl/ssd/Document.n3#Specification> .





