###  http://www.daml.org/2001/03/daml+oil#Ontology

<http://www.daml.org/2001/03/daml+oil#Ontology> owl:disjointWith :DatabaseEntry ,
                                                                 :Document ,
                                                                 <http://www.nist.gov/itl/ssd/Document.n3#Agreement> .



###  http://www.nist.gov/itl/ssd/Document#AcademicDegree

:AcademicDegree rdf:type owl:Class ;
                
                rdfs:subClassOf or:AbstractStructuredInformationStore ;
                
                owl:disjointWith <http://www.nist.gov/itl/ssd/Document.n3#Agreement> .



###  http://www.nist.gov/itl/ssd/Document#AssociatesDegree

:AssociatesDegree rdf:type owl:Class ;
                  
                  rdfs:subClassOf :AcademicDegree .



###  http://www.nist.gov/itl/ssd/Document#BachelorOfArtsDegree

:BachelorOfArtsDegree rdf:type owl:Class ;
                      
                      rdfs:subClassOf :BachelorsDegree ;
                      
                      rdfs:comment "A Bachelor's of Arts (BA) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#BachelorOfScienceDegree

:BachelorOfScienceDegree rdf:type owl:Class ;
                         
                         rdfs:subClassOf :BachelorsDegree ;
                         
                         rdfs:comment "A Bachelor's of Science (BS) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#BachelorsDegree

:BachelorsDegree rdf:type owl:Class ;
                 
                 rdfs:subClassOf :AcademicDegree .



###  http://www.nist.gov/itl/ssd/Document#Book

:Book rdf:type owl:Class ;
      
      rdfs:subClassOf :Document ;
      
      owl:disjointWith :TechnicalArticle .



###  http://www.nist.gov/itl/ssd/Document#BookSeries

:BookSeries rdf:type owl:Class ;
            
            rdfs:subClassOf :DocumentSeries .



###  http://www.nist.gov/itl/ssd/Document#ConferencePaper

:ConferencePaper rdf:type owl:Class ;
                 
                 rdfs:subClassOf :TechnicalArticle .



###  http://www.nist.gov/itl/ssd/Document#DDSDegree

:DDSDegree rdf:type owl:Class ;
           
           rdfs:subClassOf :DoctoralDegree ;
           
           rdfs:comment "A Doctor of Dental Science or Doctor of Dental Surgery (DDS) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#DatabaseEntry

:DatabaseEntry rdf:type owl:Class ;
               
               rdfs:subClassOf or:AbstractStructuredInformationStore ;
               
               owl:disjointWith :Prediction ,
                                :SystemModel ,
                                or:ComputerFile ,
                                or:DataBase ,
                                or:DataFormat ,
                                or:ImageSequence ;
               
               rdfs:comment "A Database Entry is a single entry in a Database."@en .



###  http://www.nist.gov/itl/ssd/Document#Dissertation

:Dissertation rdf:type owl:Class ;
              
              rdfs:subClassOf :Document ;
              
              owl:disjointWith :SpecificationDocument ;
              
              rdfs:comment "A dissertation or thesis.  Often publishted as a book.  It may include previously published technical articles in appendices."@en .



###  http://www.nist.gov/itl/ssd/Document#DoctorOfJurisprudenceDegree

:DoctorOfJurisprudenceDegree rdf:type owl:Class ;
                             
                             rdfs:subClassOf :DoctoralDegree ;
                             
                             rdfs:comment "A doctor of jurisprudence (JD - Juris Doctor) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#DoctorOfOsteopothyDegree

:DoctorOfOsteopothyDegree rdf:type owl:Class ;
                          
                          rdfs:subClassOf :DoctoralDegree ;
                          
                          rdfs:comment "A Doctor of Osteopothy (DO) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#DoctoralDegree

:DoctoralDegree rdf:type owl:Class ;
                
                rdfs:subClassOf :GraduateDegree .



###  http://www.nist.gov/itl/ssd/Document#Document

:Document rdf:type owl:Class ;
          
          rdfs:subClassOf or:AbstractStructuredInformationStore ;
          
          owl:disjointWith :SystemModel ,
                           <http://www.nist.gov/itl/ssd/Document.n3#Agreement> ,
                           or:ComputerFile ,
                           or:DataBase ,
                           or:DataFormat ,
                           or:ImageSequence ;
          
          rdfs:comment """The class of documents as abstract informational entities.  A single document may be instantiated into an unlimited number of copies, which may have different characteristics (font, hand-written notes, physical locations, owners, ...) which are not characteristics of the abstract document. 

Same as cyc:Document-CW"""@en .



###  http://www.nist.gov/itl/ssd/Document#DocumentCompilation

:DocumentCompilation rdf:type owl:Class ;
                     
                     rdfs:subClassOf :Document ;
                     
                     owl:disjointWith :Standard ,
                                      :TechnicalArticle ,
                                      :TechnicalReport .



###  http://www.nist.gov/itl/ssd/Document#DocumentSeries

:DocumentSeries rdf:type owl:Class ;
                
                rdfs:subClassOf or:AbstractStructuredInformationStore ;
                
                owl:disjointWith <http://www.nist.gov/itl/ssd/Document.n3#Agreement> ;
                
                rdfs:comment "A document series may be a book series or a periodical series."@en .



###  http://www.nist.gov/itl/ssd/Document#GenericSystemModel

:GenericSystemModel rdf:type owl:Class ;
                    
                    rdfs:subClassOf :SystemModel ;
                    
                    owl:disjointWith :Prediction ,
                                     :SpecificSystemModel ;
                    
                    rdfs:comment "A Generic System Model is a model for a general class of situations, contrasting with a Specific System Model which models a specific (type of) situation."@en .



###  http://www.nist.gov/itl/ssd/Document#GraduateDegree

:GraduateDegree rdf:type owl:Class ;
                
                rdfs:subClassOf :AcademicDegree .



###  http://www.nist.gov/itl/ssd/Document#JournalArticle

:JournalArticle rdf:type owl:Class ;
                
                rdfs:subClassOf :Document ;
                
                owl:disjointWith :SpecificationDocument .



###  http://www.nist.gov/itl/ssd/Document#JournalIssue

:JournalIssue rdf:type owl:Class ;
              
              rdfs:subClassOf :DocumentCompilation ;
              
              owl:disjointWith :Standard .



###  http://www.nist.gov/itl/ssd/Document#LicentiateDegree

:LicentiateDegree rdf:type owl:Class ;
                  
                  rdfs:subClassOf :GraduateDegree .



###  http://www.nist.gov/itl/ssd/Document#MDDegree

:MDDegree rdf:type owl:Class ;
          
          rdfs:subClassOf :DoctoralDegree ;
          
          rdfs:comment "A Doctor of Medicine (MD) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#MakingAPrediction

:MakingAPrediction rdf:type owl:Class ;
                   
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:intersectionOf ( or:IntentionalCreation
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty or:outputCreated ;
                                                                owl:someValuesFrom :Prediction
                                                              ]
                                                            )
                                       ] ;
                   
                   rdfs:subClassOf or:IntentionalCreation ;
                   
                   owl:disjointWith <http://www.nist.gov/itl/ssd/Document.n3#RefiningAConcept> ,
                                    or:Designing ,
                                    or:Manufacturing ;
                   
                   rdfs:comment """Each instance of MakingAPrediction is an Action in which an agent makes an assertion about what will be the case at some point in the future if some certain initial state exists. 

Same as cyc:MakingAPrediction."""@en .



###  http://www.nist.gov/itl/ssd/Document#MasterOfArtsDegree

:MasterOfArtsDegree rdf:type owl:Class ;
                    
                    rdfs:subClassOf :MastersDegree ;
                    
                    rdfs:comment "A Master of Arts (MA) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#MasterOfBusinessAdministrationDegree

:MasterOfBusinessAdministrationDegree rdf:type owl:Class ;
                                      
                                      rdfs:subClassOf :MastersDegree ;
                                      
                                      rdfs:comment "A Master of Business Administration (MBA) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#MasterOfScienceDegree

:MasterOfScienceDegree rdf:type owl:Class ;
                       
                       rdfs:subClassOf :MastersDegree ;
                       
                       rdfs:comment "A Master of Science (MS) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#MastersDegree

:MastersDegree rdf:type owl:Class ;
               
               rdfs:subClassOf :GraduateDegree .



###  http://www.nist.gov/itl/ssd/Document#NaturalLanguage

:NaturalLanguage rdf:type owl:Class ;
                 
                 rdfs:subClassOf or:Language .



###  http://www.nist.gov/itl/ssd/Document#PeriodicalSeries

:PeriodicalSeries rdf:type owl:Class ;
                  
                  rdfs:subClassOf :DocumentSeries .



###  http://www.nist.gov/itl/ssd/Document#PhDDegree

:PhDDegree rdf:type owl:Class ;
           
           rdfs:subClassOf :DoctoralDegree ;
           
           rdfs:comment "A Doctor of Philosophy (PhD) degree"@en .



###  http://www.nist.gov/itl/ssd/Document#Prediction

:Prediction rdf:type owl:Class ;
            
            rdfs:subClassOf or:AbstractStructuredInformationStore ;
            
            rdfs:comment """Each instance of Prediction is the information resulting from MakingAPrediction. Instances are forecasts of future events or situations proposed by some agent.

Same as cyc:Prediction-CW."""@en .



###  http://www.nist.gov/itl/ssd/Document#Proceedings

:Proceedings rdf:type owl:Class ;
             
             rdfs:subClassOf :DocumentCompilation .



###  http://www.nist.gov/itl/ssd/Document#Publisher

:Publisher rdf:type owl:Class ;
           
           rdfs:subClassOf or:Organization ;
           
           rdfs:comment "A Publisher is an organization which publishes material."@en .



###  http://www.nist.gov/itl/ssd/Document#ScDDegree

:ScDDegree rdf:type owl:Class ;
           
           rdfs:subClassOf :DoctoralDegree ;
           
           rdfs:comment "A Doctor of Science (ScD) Degree"@en .



###  http://www.nist.gov/itl/ssd/Document#ScientificConference

:ScientificConference rdf:type owl:Class ;
                      
                      rdfs:subClassOf or:PurposefulAction ,
                                      <http://www.nist.gov/itl/ssd/OntologyRequirements.owl#PhysicalEvent> ;
                      
                      owl:disjointWith <http://www.nist.gov/itl/ssd/Document.n3#RefiningAConcept> ,
                                       or:Recommending ;
                      
                      rdfs:comment "A scientific conferenceis a meeting for researchers to present and discuss their work.  Conferences provide an important channel for exchange of information between researchers."@en .



###  http://www.nist.gov/itl/ssd/Document#SpecificSystemModel

:SpecificSystemModel rdf:type owl:Class ;
                     
                     rdfs:subClassOf :SystemModel ;
                     
                     rdfs:comment "A Specific System Model is a model for a specific (type of) situation, contrasting with a Generic System Model which models a general class of situations."@en .



###  http://www.nist.gov/itl/ssd/Document#SpecificationDocument

:SpecificationDocument rdf:type owl:Class ;
                       
                       rdfs:subClassOf :Document ;
                       
                       owl:disjointWith :TechnicalArticle ,
                                        :TechnicalReport .



###  http://www.nist.gov/itl/ssd/Document#Standard

:Standard rdf:type owl:Class ;
          
          rdfs:subClassOf :Document ;
          
          owl:disjointWith :TechnicalArticle ,
                           :TechnicalReport .



###  http://www.nist.gov/itl/ssd/Document#SystemModel

:SystemModel rdf:type owl:Class ;
             
             rdfs:subClassOf or:ConceptualModel ;
             
             owl:disjointWith or:ComputerFile ,
                              or:DataBase ,
                              or:DataFormat ,
                              or:ImageSequence ;
             
             rdfs:comment "A System Model is a conceptual model of how a physical, social, or computaional system acts, describing the types of things which are modeled, relationships among things which are modeled, and results to be expected from different types of events.  Specific models may describe other classes of information as well."@en .



###  http://www.nist.gov/itl/ssd/Document#TechnicalArticle

:TechnicalArticle rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Document ;
                  
                  owl:disjointWith :TechnicalReport .



###  http://www.nist.gov/itl/ssd/Document#TechnicalJournalArticle

:TechnicalJournalArticle rdf:type owl:Class ;
                         
                         owl:equivalentClass [ rdf:type owl:Class ;
                                               owl:intersectionOf ( :JournalArticle
                                                                    :TechnicalArticle
                                                                  )
                                             ] ;
                         
                         rdfs:subClassOf :JournalArticle .



###  http://www.nist.gov/itl/ssd/Document#TechnicalJournalSeries

:TechnicalJournalSeries rdf:type owl:Class ;
                        
                        rdfs:subClassOf :PeriodicalSeries .



###  http://www.nist.gov/itl/ssd/Document#TechnicalReport

:TechnicalReport rdf:type owl:Class ;
                 
                 rdfs:subClassOf :Document .



###  http://www.nist.gov/itl/ssd/Document.n3#Agreement

<http://www.nist.gov/itl/ssd/Document.n3#Agreement> rdf:type owl:Class ;
                                                    
                                                    rdfs:subClassOf <http://www.nist.gov/itl/ssd/Document.n3#Specification> ;
                                                    
                                                    owl:disjointWith or:Array ,
                                                                     or:DataFormat ,
                                                                     or:ImageSequence ,
                                                                     or:MathematicalExpression ;
                                                    
