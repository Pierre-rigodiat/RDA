                                             rdfs:comment "The temperature (in degrees C) at which the extracellular  matrix was incubated."@en ;
                                             
                                             rdfs:domain exp:BiologicalAssay ;
                                             
                                             rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#frozenStorageDate

exp:frozenStorageDate rdf:type owl:DatatypeProperty ;
                      
                      rdfs:comment "The `frozen Storage Date' is the date the culture was frozen."@en ;
                      
                      rdfs:domain exp:CellCulture ;
                      
                      rdfs:range xsd:dateTime .



