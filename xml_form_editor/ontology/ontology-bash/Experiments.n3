###  http://www.nist.gov/itl/ssd/Experiments#assayChamberFormat

exp:assayChamberFormat rdf:type owl:ObjectProperty ;
                       
                       rdfs:comment "The type of chamber format that is being used for cell assay"@en ;
                       
                       rdfs:range exp:CellAssayChamberFormat ;
                       
                       rdfs:domain exp:CellImagingExperiment .



###  http://www.nist.gov/itl/ssd/Experiments#assayChamberSubstrate

exp:assayChamberSubstrate rdf:type owl:ObjectProperty ;
                          
                          rdfs:comment "The material used to prepare the cell adhesion substrate of the assay chamber."@en ;
                          
                          rdfs:domain exp:CellImagingExperiment ;
                          
                          rdfs:range or:MaterialType ;
                          
                          rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.nist.gov/itl/ssd/Experiments#cellTypeOfCulture

exp:cellTypeOfCulture rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "'cell Type Of Culture' maps a Cell Type to a Cell Culture which is composed of cells of the given type."@en ;
                      
                      rdfs:domain bc:CellType ;
                      
                      rdfs:range exp:CellCulture .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumAminoAcidSupplement

exp:cultureMediumAminoAcidSupplement rdf:type owl:ObjectProperty ;
                                     
                                     rdfs:comment "The `culture Medium Amino Acid Supplement' is an Amino Acid Supplement Type used in the maintenance Cell Culture.  A given Cell Culture may use more than one type of amino acid supplement."@en ;
                                     
                                     rdfs:range exp:AminoAcidSupplementType ;
                                     
                                     rdfs:domain exp:CultureMedium .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumAntibioticSupplement

exp:cultureMediumAntibioticSupplement rdf:type owl:ObjectProperty ;
                                      
                                      rdfs:comment "The `culture Medium Antibiotic Supplement' is an Antibiotic Type used in the maintenance Cell Culture.  A given Cell Culture may use more than one type of antibiotic."@en ;
                                      
                                      rdfs:range exp:AntibioticType ;
                                      
                                      rdfs:domain exp:CellCulture .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumHormoneSupplement

exp:cultureMediumHormoneSupplement rdf:type owl:ObjectProperty ;
                                   
                                   rdfs:comment "The `culture Medium Hormone Supplement' is an Hormone Type used in the maintenance Cell Culture.  A given Cell Culture may use more than one type of hormone."@en ;
                                   
                                   rdfs:domain exp:CultureMedium ;
                                   
                                   rdfs:range exp:HormoneType .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumSerumType

exp:cultureMediumSerumType rdf:type owl:FunctionalProperty ,
                                    owl:ObjectProperty ;
                           
                           rdfs:comment "The `culture Medium Serum Supplement' is a Serum Type used in the maintenance Cell Culture.  A given Cell Culture uses a single type of serum."@en ;
                           
                           rdfs:domain exp:CellCulture ;
                           
                           rdfs:range exp:SerumType .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumUsed

exp:cultureMediumUsed rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain exp:CellCulture ;
                      
                      rdfs:range exp:CultureMediumType .



###  http://www.nist.gov/itl/ssd/Experiments#cultureSubstrate

exp:cultureSubstrate rdf:type owl:FunctionalProperty ,
                              owl:ObjectProperty ;
                     
                     rdfs:comment "The property `culture Substrate' specifies the type of adhesion surface for routine passaging of the maintenance culture (e.g., Tissue Culture Polystyrene of Gelatin)."@en ;
                     
                     rdfs:range exp:AdhesionSurfaceType ;
                     
                     rdfs:domain exp:CellCulture .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixDepositionSolvent

exp:extracellularMatrixDepositionSolvent rdf:type owl:ObjectProperty ;
                                         
                                         rdfs:comment "The solvent used in extracellular matrix deposition."@en ;
                                         
                                         rdfs:domain exp:BiologicalAssay ;
                                         
                                         rdfs:range exp:SolventType .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixMolecularStructure

exp:extracellularMatrixMolecularStructure rdf:type owl:ObjectProperty ;
                                          
                                          rdfs:comment "the structure of the proteins that are used to form the extracellular matrix"@en ;
                                          
                                          rdfs:range bc:ProteinTypeByStructure ;
                                          
                                          rdfs:domain exp:BiologicalAssay .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixOtherComponents

exp:extracellularMatrixOtherComponents rdf:type owl:ObjectProperty ;
                                       
                                       <http://www.w3.org/2004/02/skos/core#note> "``protein, nonprotein additives'' used for Cell Assays."@en ;
                                       
                                       rdfs:domain exp:BiologicalAssay ;
                                       
                                       rdfs:range or:MaterialType .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixPatterning

exp:extracellularMatrixPatterning rdf:type owl:ObjectProperty ;
                                  
                                  <http://www.w3.org/2004/02/skos/core#note> "What does this mean?"@en ;
                                  
                                  rdfs:domain exp:BiologicalAssay ;
                                  
                                  rdfs:range exp:ExtracellularMatrixPatterningType .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixTreatment

exp:extracellularMatrixTreatment rdf:type owl:ObjectProperty ;
                                 
                                 <http://www.w3.org/2004/02/skos/core#note> "what does this mean?"@en ;
                                 
                                 rdfs:domain exp:BiologicalAssay ;
                                 
                                 rdfs:range exp:ExtracellularMatrixTreatmentType .



###  http://www.nist.gov/itl/ssd/Experiments#imageProducedByStudy

exp:imageProducedByStudy rdf:type owl:ObjectProperty ;
                         
                         rdfs:range exp:ExperimentalStudy ;
                         
                         rdfs:domain or:Image .



###  http://www.nist.gov/itl/ssd/Experiments#imagingDeviceUsedInStudy

exp:imagingDeviceUsedInStudy rdf:type owl:ObjectProperty ;
                             
                             rdfs:range exp:ExperimentalStudy ;
                             
                             rdfs:domain or:ImagingDevice .



###  http://www.nist.gov/itl/ssd/Experiments#investigator

exp:investigator rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "Any researcher for a research study.  Use the property 'principal Investigator' if appropriate."@en ;
                 
                 rdfs:domain exp:Research ;
                 
                 rdfs:range or:Human .



###  http://www.nist.gov/itl/ssd/Experiments#measurementResult

exp:measurementResult rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "The property 'measurement Result' relates an act of Measuring to an Expressed Quantity."@en ;
                      
                      rdfs:domain exp:Measuring ;
                      
                      rdfs:range or:ExpressedQuantity .



###  http://www.nist.gov/itl/ssd/Experiments#measurementTechniqueUsed

exp:measurementTechniqueUsed rdf:type owl:ObjectProperty ;
                             
                             rdfs:comment "The property 'measurement Technique Used' relates an act of Measuring something to the Measurement Technique that it implemented."@en ;
                             
                             rdfs:range exp:MeasurementTechnique ;
                             
                             rdfs:domain exp:Measuring .



###  http://www.nist.gov/itl/ssd/Experiments#numOfSignificantDigits

exp:numOfSignificantDigits rdf:type owl:FunctionalProperty ,
                                    owl:ObjectProperty ;
                           
                           rdfs:comment "The property 'num Of Significant Digits' specifies the Number Of Significant Digits of a measurement or computed value."@en ;
                           
                           rdfs:range exp:NumberOfSignificantDigits ;
                           
                           rdfs:subPropertyOf exp:uncertaintyOfQuantity ;
                           
                           rdfs:domain or:ExpressedQuantity .



###  http://www.nist.gov/itl/ssd/Experiments#organismTypeOfCulture

exp:organismTypeOfCulture rdf:type owl:FunctionalProperty ,
                                   owl:ObjectProperty ;
                          
                          rdfs:comment "The `organism Type Of Culture' for some Cell Culture is the species of organism from which the cells were obtained.  This may be either s species of Single-Celled Organisms or Multi-Celled Organism."@en ;
                          
                          rdfs:range bc:BiologicalSpecies ;
                          
                          rdfs:domain exp:CellCulture .



###  http://www.nist.gov/itl/ssd/Experiments#preseedingDissociationMethod

exp:preseedingDissociationMethod rdf:type owl:ObjectProperty ;
                                 
                                 rdfs:comment "The dissociation method used immediately prior to seeding for experiment."@en ;
                                 
                                 rdfs:domain exp:BiologicalAssay ;
                                 
                                 rdfs:range exp:CellDissociationType .



###  http://www.nist.gov/itl/ssd/Experiments#principalInvestigator

exp:principalInvestigator rdf:type owl:ObjectProperty ;
                          
                          rdfs:comment "The designer of a research study."@en ;
                          
                          rdfs:domain exp:Research ;
                          
                          rdfs:subPropertyOf exp:investigator ;
                          
                          rdfs:range or:Human .



###  http://www.nist.gov/itl/ssd/Experiments#principleProteinComponent

exp:principleProteinComponent rdf:type owl:ObjectProperty ;
                              
                              rdfs:comment "The prinicple protein component of the extracellular matrix."@en ;
                              
                              rdfs:range bc:ProteinTypeBySpecies ;
                              
                              rdfs:domain exp:ExtracellularMatrix .



###  http://www.nist.gov/itl/ssd/Experiments#researchUsesSoftware

exp:researchUsesSoftware rdf:type owl:ObjectProperty ;
                         
                         rdfs:comment "The specified research uses the specified software for data processing.  If it merely is performing research on the software, this predicate does not apply."@en ;
                         
                         rdfs:domain exp:Research ;
                         
                         rdfs:range or:Software ;
                         
                         rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.nist.gov/itl/ssd/Experiments#sampleFromPatient

exp:sampleFromPatient rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "The specified sample is from the body of the patient.  It may be tissue or a blood sample that was formerly part of the patient, tissue that was grown from the patient's tissue, an infectious agent, or other matter.  The ``sample' ' may also be an existing part of the patient that was x-rayed or otherwise imaged."@en ;
                      
                      rdfs:range or:Human ;
                      
                      rdfs:domain or:PhysicalObject ;
                      
                      rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.nist.gov/itl/ssd/Experiments#standardDeviation

exp:standardDeviation rdf:type owl:FunctionalProperty ,
                               owl:ObjectProperty ;
                      
                      rdfs:comment "The standard Deviation of an Uncertainty is a quantity with the same units as the Uncertainty itself."@en ;
                      
                      rdfs:domain exp:Uncertainty ;
                      
                      rdfs:range or:Quantity .



###  http://www.nist.gov/itl/ssd/Experiments#technician

exp:technician rdf:type owl:ObjectProperty ;
               
               rdfs:comment "The person who performs a technical task as part of some scientific study or similar technical activity."@en ;
               
               rdfs:range or:Human ;
               
               rdfs:domain or:PurposefulAction ;
               
               rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfProperty

exp:uncertaintyOfProperty rdf:type owl:ObjectProperty ;
                          
                          rdfs:range exp:Uncertainty .



###  http://www.nist.gov/itl/ssd/Experiments#uncertaintyOfQuantity

exp:uncertaintyOfQuantity rdf:type owl:ObjectProperty ;
                          
                          rdfs:comment "The property 'uncertainty Of Quantity' relates a Quantity to its associated Uncertainty."@en ;
                          
                          rdfs:range exp:Uncertainty ;
                          
                          rdfs:domain or:MeasurableQuantity .



###  http://www.nist.gov/itl/ssd/Experiments.n3#hasUncertaintyDistributionType

<http://www.nist.gov/itl/ssd/Experiments.n3#hasUncertaintyDistributionType> rdf:type owl:FunctionalProperty ,
                                                                                     owl:ObjectProperty ;
                                                                            
                                                                            rdfs:comment "The property 'has Uncertainty Distribution Type' relates an Uncertainty to its Uncertainty Distribution Type, e.g., Gaussian Distribution."@en ;
                                                                            
                                                                            rdfs:domain exp:Uncertainty ;
                                                                            
                                                                            rdfs:range <http://www.nist.gov/itl/ssd/Experiments.n3#UncertaintyDistributionType> .



###  http://www.nist.gov/itl/ssd/OntologyRequirements#implementsAlgorithm

or:implementsAlgorithm rdf:type owl:ObjectProperty .



###  http://www.nist.gov/itl/ssd/OntologyRequirements#outputData

or:outputData rdf:type owl:ObjectProperty .





