              rdfs:domain or:Artifact ;
              
              rdfs:subPropertyOf or:nameString ;
              
              rdfs:range xsd:NMTOKEN .



###  http://www.nist.gov/itl/ssd/Experiments#maintenancePlatingDensity

exp:maintenancePlatingDensity rdf:type owl:DatatypeProperty ;
                              
