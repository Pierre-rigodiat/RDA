###  http://www.nist.gov/itl/ssd/Experiments#meanCellDoublingTimeInHours

exp:meanCellDoublingTimeInHours rdf:type owl:DatatypeProperty ;
                                
                                rdfs:comment "The measured mean cell doubling time (in hours)"@en ;
                                
                                rdfs:domain exp:CellCulture ;
                                
                                rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#meanCellSpreadAreaInSqMicrons

exp:meanCellSpreadAreaInSqMicrons rdf:type owl:DatatypeProperty ;
                                  
