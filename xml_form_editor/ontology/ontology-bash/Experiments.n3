                                  rdfs:domain exp:CellCulture ;
                                  
                                  rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#meanCellVolumeInCubicMicrons

exp:meanCellVolumeInCubicMicrons rdf:type owl:DatatypeProperty ;
                                 
                                 rdfs:domain exp:CellCulture ;
                                 
                                 rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#passBandCenterNM

exp:passBandCenterNM rdf:type owl:DatatypeProperty ;
                     
                     rdfs:comment "The property `pass Band Center NM' specifies the center of a Band Pass Light Filter in nanometers."@en ;
                     
                     rdfs:domain exp:BandPassLightFilter ;
                     
                     rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#passBandHalfWidthNM

exp:passBandHalfWidthNM rdf:type owl:DatatypeProperty ;
                        
