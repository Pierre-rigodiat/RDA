###  http://www.nist.gov/itl/ssd/Experiments#assayChamberTemperatureC

exp:assayChamberTemperatureC rdf:type owl:DatatypeProperty ;
                             
                             rdfs:comment "The temperature (in degrees C) of the Assay Chamber temperature in a Cell Imaging Experiment."@en ;
                             
                             rdfs:domain exp:CellImagingExperiment ;
                             
                             rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#assayPassageNumber

exp:assayPassageNumber rdf:type owl:DatatypeProperty ;
                       
                       rdfs:comment "The number of times the cells have been passaged at the time they are used in the experiment."@en ;
                       
                       rdfs:domain exp:BiologicalAssay ;
                       
                       rdfs:range xsd:nonNegativeInteger .



###  http://www.nist.gov/itl/ssd/Experiments#assaySeedingDensityPerCm2

exp:assaySeedingDensityPerCm2 rdf:type owl:DatatypeProperty ;
                              
                              rdfs:comment "The cell seeding density for the experiment in terms of cells per square cm."@en ;
                              
                              rdfs:domain exp:BiologicalAssay ;
                              
                              rdfs:range xsd:positiveInteger .



###  http://www.nist.gov/itl/ssd/Experiments#cellDesignatiion

exp:cellDesignatiion rdf:type owl:DatatypeProperty ;
                     
                     rdfs:comment "A `cell Designation' names the cell line  (?) in a Cell Culture."@en ;
                     
                     rdfs:domain exp:CellCulture ;
                     
                     rdfs:range xsd:NMTOKEN .



###  http://www.nist.gov/itl/ssd/Experiments#cellModification

exp:cellModification rdf:type owl:DatatypeProperty ;
                     
                     rdfs:comment """`cell Modification' names a type of modification done on a cell culture.  Types include electroporation, iontophoresis, pneumatic injectors, and mechanical
injectors, and use of liposomes."""@en ;
                     
                     rdfs:domain exp:CellCulture ;
                     
                     rdfs:range xsd:NMTOKEN .



###  http://www.nist.gov/itl/ssd/Experiments#cultureCO2Percent

exp:cultureCO2Percent rdf:type owl:DatatypeProperty ;
                      
                      rdfs:comment "The `culture CO2 Percent' is the percentage of CO2 in the maintenance culture's cell incubator."@en ;
                      
                      rdfs:domain exp:CellCulture ;
                      
                      rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMaintenanceTemperatureC

exp:cultureMaintenanceTemperatureC rdf:type owl:DatatypeProperty ;
                                   
                                   rdfs:comment "The `culture Maintenance Temperature C' is the temperature (in degrees C) at which the maintenace culture was incubated."@en ;
                                   
                                   rdfs:domain exp:CellCulture ;
                                   
                                   rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumName

exp:cultureMediumName rdf:type owl:DatatypeProperty ;
                      
                      rdfs:comment "? Is there a limited number of cell cultures, so that they should be reified instead of merely their names being specified."@en ,
                                   "The `culture Medium Name' of a Cell Culture is the name of the culture used in a maintenance Cell Culture.  "@en ;
                      
                      rdfs:domain exp:CellCulture ;
                      
                      rdfs:range xsd:NMTOKEN .



###  http://www.nist.gov/itl/ssd/Experiments#cultureMediumSerumPercent

exp:cultureMediumSerumPercent rdf:type owl:DatatypeProperty ;
                              
                              rdfs:comment "The percent of volume of serum in the maintenance culture media"@en ;
                              
                              rdfs:domain exp:CultureMedium ;
                              
                              rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#cultureVesselName

exp:cultureVesselName rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      
                      rdfs:comment "The `culture Vessel Name' is the name of the container (type) used for routine passaging of the maintenance culture."@en ;
                      
                      rdfs:domain exp:MaintenanceCellCulture ;
                      
                      rdfs:range xsd:NMTOKEN .



###  http://www.nist.gov/itl/ssd/Experiments#depositionExtracellularMatrixSolutionConcGPL

exp:depositionExtracellularMatrixSolutionConcGPL rdf:type owl:DatatypeProperty ;
                                                 
                                                 rdfs:comment "The extracellular matrix solution concentration during deposition in mg/ml."@en ;
                                                 
                                                 rdfs:domain exp:BiologicalAssay ;
                                                 
                                                 rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixIncubationTimeInHours

exp:extracellularMatrixIncubationTimeInHours rdf:type owl:DatatypeProperty ;
                                             
                                             rdfs:comment "The number of hours of incubation for an extracellular matrix."@en ;
                                             
                                             rdfs:domain exp:BiologicalAssay ;
                                             
                                             rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#extracellularMatrixSolutionConcMgPerMl

exp:extracellularMatrixSolutionConcMgPerMl rdf:type owl:DatatypeProperty ;
                                           
                                           rdfs:domain exp:BiologicalAssay ;
                                           
                                           rdfs:range xsd:decimal .



###  http://www.nist.gov/itl/ssd/Experiments#extracellulrMatrixIncubationTemperatureC

